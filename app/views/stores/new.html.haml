.contents.row
  .container
    %h3
      投稿する
    .form
      = form_for @stores do |f|
        %p
          = f.label :name
          = f.text_field :name
        %p
          = f.label :address
          = f.text_field :address
        %p
          = f.label :text
          = f.text_area :text
        = f.submit
